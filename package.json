    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.9",
    "metalsmith": "2.3.0",
    "metalsmith-collections": "0.9.0",
    "metalsmith-layouts": "2.1.0",
    "metalsmith-markdown": "0.2.1",
    "minami": "1.2.3",
