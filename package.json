    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-istanbul": "4.1.6",
    "babel-preset-es2015": "6.24.1",
    "coveralls": "3.0.0",
    "eslint": "4.19.1",
