    "chocolat": "^0.4.21",
    "cleave.js": "^1.4.7",
    "codemirror": "^5.43.0",
    "css-loader": "^2.1.1",
    "datatables": "^1.10.18",
    "datatables.net-responsive-bs4": "^2.2.3",
    "datatables.net-select-bs4": "^1.2.7",
    "dropzone": "^5.5.1",
    "file-loader": "^3.0.1",
    "flag-icon-css": "^3.2.1",
    "fullcalendar": "^3.10.0",
    "gmaps": "^0.4.24",
    "ionicons201": "1.0.0",
    "izitoast": "^1.4.0",
    "jquery": "^3.3.1",
    "jquery-pwstrength": "https://github.com/matoilic/jquery.pwstrength",
    "jquery-sparkline": "^2.4.0",
    "jquery-ui-dist": "^1.12.1",
    "jquery.nicescroll": "^3.7.6",
